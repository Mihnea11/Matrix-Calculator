            <tr><td>c<sub>1,2</sub> = 1×6 + 2×8 + 1×1 = 23</td></tr>
            <tr><td>c<sub>1,3</sub> = 1×1 + 2×1 + 1×1 = 4</td></tr>
            <tr><td>c<sub>1,4</sub> = 1×1 + 2×1 + 1×1 = 4</td></tr>
            <tr><td>c<sub>2,1</sub> = 3×5 + 4×7 + 1×1 = 44</td></tr>
            <tr><td>c<sub>2,2</sub> = 3×6 + 4×8 + 1×1 = 51</td></tr>
            <tr><td>c<sub>2,3</sub> = 3×1 + 4×1 + 1×1 = 8</td></tr>
            <tr><td>c<sub>2,4</sub> = 3×1 + 4×1 + 1×1 = 8</td></tr>
        </tbody>
    </table>
